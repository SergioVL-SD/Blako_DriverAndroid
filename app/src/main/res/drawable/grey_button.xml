<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="@color/blako_gray_low" />
      <stroke
          android:width="1dp"
          android:color="@color/blako_gray_medium" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="@color/blako_gray_medium"
          android:endColor="@color/blako_gray_medium"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="@color/blako_gray_medium" />
      <corners
          android:radius="2dp" />
      <padding
          android:left="10dp"
          android:top="10dp"
          android:right="10dp"
          android:bottom="10dp" />
    </shape>
  </item>
</selector>
    

